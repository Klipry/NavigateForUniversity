<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#ABC0C5">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="105dp"
        android:layout_height="150dp"
        android:layout_marginStart="4dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:scaleType="fitEnd"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.44"
        app:srcCompat="@drawable/acthall"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="258dp"
        android:layout_height="199dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:srcCompat="@drawable/corp2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/seach" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="208dp"
        android:layout_height="243dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:srcCompat="@drawable/unionupdate" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="11dp"
        android:layout_height="9dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/iconlestn" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="9dp"
        android:layout_height="7dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/elevator" />

</androidx.constraintlayout.widget.ConstraintLayout>